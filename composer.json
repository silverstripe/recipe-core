{
    "name": "silverstripe/recipe-core",
    "type": "silverstripe-recipe",
    "description": "SilverStripe framework-only core recipe",
    "homepage": "http://silverstripe.org",
    "license": "BSD-3-Clause",
    "require": {
        "php": "^8.1",
        "silverstripe/recipe-plugin": "~2.0.1@stable",
        "silverstripe/assets": "~2.3.0@rc",
        "silverstripe/config": "~2.1.1@stable",
        "silverstripe/framework": "~5.3.0@rc",
        "silverstripe/mimevalidator": "~3.1.0@stable"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "silverstripe/versioned": "^2",
        "mikey179/vfsstream": "^1.6.11"
    },
    "extra": {
        "project-files": [
            ".htaccess",
            "app/*"
        ],
        "public-files": [
            ".htaccess",
            "web.config",
            "index.php"
        ]
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}