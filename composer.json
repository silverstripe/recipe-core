{
    "name": "silverstripe/recipe-core",
    "type": "silverstripe-recipe",
    "description": "SilverStripe framework-only core recipe",
    "homepage": "http://silverstripe.org",
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/recipe-plugin": "^1.2",
        "silverstripe/assets": "2.x-dev",
        "silverstripe/config": "2.x-dev",
        "silverstripe/framework": "5.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "7.1.3"
    },
    "extra": {
        "project-files": [
            "app/*"
        ],
        "public-files": [
            ".htaccess",
            "web.config",
            "index.php"
        ],
        "branch-alias": {
            "dev-master": "5.x-dev"
        },
        "resources-dir": "_resources"
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
